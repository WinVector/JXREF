<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns="http://www.manning.com/schemas/book"
		xmlns:xml="http://www.w3.org/XML/1998/namespace"
           xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified"
           elementFormDefault="qualified" targetNamespace="http://www.manning.com/schemas/book">
    <!-- <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">-->

    <xs:import namespace="http://www.w3.org/XML/1998/namespace"
               schemaLocation="xmlspace.xsd"/>
           
    <xs:group name="globalElements">
        <xs:choice>
            <xs:element ref="specialCharacter"/>
            <xs:element ref="indexterm"/>
            <xs:element ref="internalnote"/>
            <xs:element ref="addToIndex"/>
            <xs:element ref="unicode"/>
            <xs:element ref="info"/>
        </xs:choice>
    </xs:group>

    <xs:attributeGroup name="globalAttributes">
        <xs:attribute name="id" type="xs:ID"/>
        <xs:attribute name="xreflabel" type="xs:string"/>
        <xs:attribute name="context" type="xs:string"/>
        <xs:attribute name="lang" type="xs:string"/>
        <xs:attribute ref="xml:base"/>
    </xs:attributeGroup>

    <xs:complexType name="genericTextElement" mixed="true">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:group ref="globalElements"/>
        </xs:choice>
        <xs:attributeGroup ref="globalAttributes"/>
    </xs:complexType>

    <xs:attributeGroup name="documentationAttributes">
        <xs:attribute name="docbase" type="xs:string"/>
        <xs:attribute name="doclink" type="xs:string"/>
    </xs:attributeGroup>

    <xs:element name="info" type="infoInfo"/>
    <xs:complexType name="infoInfo">
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="type" type="infoSubElementInfo"/>
            <xs:element name="appliesTo" type="infoSubElementInfo"/>
        </xs:choice>
        <xs:attributeGroup ref="globalAttributes"/>
    </xs:complexType>

    <xs:complexType name="infoSubElementInfo" mixed="true">
            <xs:attributeGroup ref="globalAttributes"/>
    </xs:complexType>

    <xs:element name="book">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="bookinfo">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="title" type="sectionTitle"/>
                            <xs:element name="subtitle" type="genericTextElement" minOccurs="0"/>
                            <xs:element maxOccurs="unbounded" ref="author"/>
                            <xs:element ref="toc" minOccurs="0"/>
                        </xs:sequence>
                        <xs:attribute name="id" type="xs:ID"/>
                        <xs:attribute name="xreflabel" type="xs:string"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" ref="toc"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="preface"/>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="part"/>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="chapter"/>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="appendix"/>
            </xs:sequence>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="author" type="authorInfo"/>
    <xs:complexType name="authorInfo">
        <xs:choice>
            <xs:sequence>
                <xs:element name="personname" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:choice maxOccurs="4">
                            <xs:element maxOccurs="1" ref="surname"/>
                            <xs:element maxOccurs="1" minOccurs="0" ref="othername"/>
                            <xs:element maxOccurs="1" ref="firstname"/>
                        </xs:choice>
                        <xs:attributeGroup ref="globalAttributes"/>
                    </xs:complexType>
                </xs:element>
                <xs:element maxOccurs="1" minOccurs="0" name="email"
                            type="xs:string"/>
            </xs:sequence>
            <xs:sequence>
                <xs:choice maxOccurs="4">
                    <xs:element maxOccurs="1" ref="surname"/>
                    <xs:element maxOccurs="1" minOccurs="0" name="othername"/>
                    <xs:element maxOccurs="1" ref="firstname"/>
                </xs:choice>
                <xs:element maxOccurs="1" minOccurs="0" name="email"
                            type="xs:string"/>
            </xs:sequence>
        </xs:choice>
        <xs:attributeGroup ref="globalAttributes"/>
    </xs:complexType>


    <xs:element name="part">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="title" type="sectionTitle"/>
                <xs:element minOccurs="0" ref="partintro"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="chapter"/>
            </xs:sequence>
            <xs:attributeGroup ref="globalAttributes"/>
            <xs:attribute name="label" use="optional"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="partintro">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element maxOccurs="unbounded" ref="para"/>
                <xs:element maxOccurs="unbounded" ref="itemizedlist"/>
                <xs:element maxOccurs="unbounded" ref="section"/>
                <xs:element maxOccurs="unbounded" ref="internalnote"/>
            </xs:choice>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="package">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
            </xs:choice>
            <xs:attributeGroup ref="documentationAttributes"/>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="classname">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
            </xs:choice>
            <xs:attributeGroup ref="documentationAttributes"/>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="methodname">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
            </xs:choice>
            <xs:attributeGroup ref="documentationAttributes"/>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="interfacename">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
            </xs:choice>
            <xs:attributeGroup ref="documentationAttributes"/>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="property">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
            </xs:choice>
            <xs:attributeGroup ref="documentationAttributes"/>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="para">
        <xs:complexType mixed="true">
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:group ref="globalElements"/>
                <xs:element ref="property"/>
                <xs:element ref="interfacename"/>
                <xs:element ref="package"/>
                <xs:element ref="methodname"/>
                <xs:element ref="classname"/>
                <xs:element ref="code"/>
                <xs:element ref="emphasis"/>
                <xs:element ref="xref"/>
                <xs:element ref="figure"/>
                <xs:element ref="informalfigure"/>
                <xs:element ref="itemizedlist"/>
                <xs:element ref="variablelist"/>
                <xs:element ref="informalexample"/>
                <xs:element ref="orderedlist"/>
                <xs:element ref="table"/>
                <xs:element ref="example"/>
                <xs:element ref="glossterm"/>
                <xs:element ref="footnote"/>
                <xs:element ref="blockquote"/>
                <xs:element ref="ulink"/>
                <xs:element ref="uri"/>
                <xs:element ref="filename"/>
                <xs:element ref="inlineequation"/>
                <xs:element ref="quote"/>
                <xs:element ref="citation"/>
                <xs:element ref="command"/>
                <xs:element ref="computeroutput"/>
                <xs:element ref="userinput"/>
                <xs:element ref="programlistingco"/>
                <xs:element ref="mediaobject"/>
                <xs:element ref="citetitle"/>
                <xs:element ref="informalequation"/>
                <xs:element ref="informaltable"/>
                <xs:element ref="subscript"/>
                <xs:element ref="superscript"/>
                <xs:element ref="co"/>
                <xs:element ref="acronym"/>
                <xs:element ref="link"/>
                <xs:element ref="inlinemediaobject"/>
                <xs:element ref="biblioref"/>
            </xs:choice>
            <xs:attribute name="role" type="xs:string"/>
            <xs:attribute name="target" type="xs:string"/>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="computeroutput">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
            </xs:choice>
            <xs:attributeGroup ref="documentationAttributes"/>
            <xs:attributeGroup ref="globalAttributes"/>
            <xs:attribute name="moreinfo" type="xs:string"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="userinput">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
            </xs:choice>
            <xs:attributeGroup ref="documentationAttributes"/>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="command">
        <xs:complexType mixed="true">
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="codelink"/>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="emphasis"/>
                <xs:group ref="globalElements"/>
            </xs:choice>
            <xs:attributeGroup ref="documentationAttributes"/>
            <xs:attributeGroup ref="globalAttributes"/>
            <xs:attribute name="moreinfo" type="xs:string"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="codelink">
        <xs:complexType>
            <xs:attribute name="file" type="xs:string"/>
            <xs:attribute name="linkend" type="xs:string" use="optional"/>

            <xs:attribute name="commentStyle" type="xs:string" use="optional"/>
            <xs:attribute name="leadingChars" type="xs:string" use="optional"/>
            <xs:attribute name="trailingChars" type="xs:string" use="optional"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="variablelist">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="varlistentry"/>
            </xs:sequence>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="varlistentry">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="term"/>
                <xs:element ref="listitem"/>
            </xs:sequence>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="term">
        <xs:complexType mixed="true">
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:group ref="globalElements"/>
                <xs:element minOccurs="0" ref="uri"/>
                <xs:element minOccurs="0" ref="citetitle"/>
                <xs:element minOccurs="0" ref="code"/>
                <xs:element minOccurs="0" ref="command"/>
                <xs:element minOccurs="0" name="varname" type="xs:string"/>
                <xs:element minOccurs="0" ref="filename"/>
            </xs:choice>
            <xs:attributeGroup ref="documentationAttributes"/>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="xref">
        <xs:complexType>
            <xs:attribute name="linkend" type="xs:IDREF" use="required"/>
            <xs:attribute name="endterm" type="xs:IDREF"/>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="quote">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
            </xs:choice>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="blockquote">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:group ref="globalElements"/>
                <xs:element ref="para"/>
                <xs:element ref="attribution"/>
            </xs:choice>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="attribution">
        <xs:complexType mixed="true">
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:group ref="globalElements"/>
                <xs:element ref="para" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="emphasis" minOccurs="0" maxOccurs="unbounded"/>
            </xs:choice>

            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="addToIndex">
        <xs:complexType mixed="true">
<xs:choice><xs:any minOccurs="0" maxOccurs="unbounded" /></xs:choice>
            <xs:attributeGroup ref="globalAttributes"/>
            <xs:attribute name="hidden" type="xs:string"/>
            <xs:attribute name="secondary" type="xs:string"/>
            <xs:attribute name="tertiary" type="xs:string"/>
            <xs:attribute name="seeAlso" type="xs:string"/>

        </xs:complexType>
    </xs:element>

    <xs:element name="addSecondaryToIndex">
        <xs:complexType mixed="true">
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="addTertiaryToIndex">
        <xs:complexType mixed="true">
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="filename">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
            </xs:choice>
            <xs:attributeGroup ref="documentationAttributes"/>
            <xs:attributeGroup ref="globalAttributes"/>
            <xs:attribute name="moreinfo" type="xs:string"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="link">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
            </xs:choice>
            <xs:attribute name="linkend" type="xs:string"/>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="ulink">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
                <xs:element minOccurs="0" maxOccurs="unbounded" ref="citation"/>
            </xs:choice>
            <xs:attribute name="url" type="xs:string"/>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="uri">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
            </xs:choice>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="glossterm">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
            </xs:choice>
            <xs:attribute name="linkend" type="xs:IDREF"/>
            <xs:attribute name="baseform" type="xs:string"/>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="code">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="emphasis"/>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="package"/>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="classname"/>
                <xs:element maxOccurs="unbounded" minOccurs="0" ref="methodname"/>
                <xs:element maxOccurs="unbounded" minOccurs="0"
                            ref="interfacename"/>
            </xs:choice>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>


    <xs:complexType name="sectionTitle" mixed="true">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:group ref="globalElements"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="package"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="classname"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="methodname"/>
            <xs:element maxOccurs="unbounded" minOccurs="0"
                        ref="interfacename"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="code"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="emphasis"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="xref"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="filename"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="superscript"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="subscript"/>
        </xs:choice>
        <xs:attributeGroup ref="globalAttributes"/>
    </xs:complexType>
    <xs:complexType name="captionTitle" mixed="true">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:group ref="globalElements"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="package"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="classname"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="methodname"/>
            <xs:element maxOccurs="unbounded" minOccurs="0"
                        ref="interfacename"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="code"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="xref"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="filename"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="emphasis"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="superscript"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="subscript"/>
        </xs:choice>
        <xs:attributeGroup ref="globalAttributes"/>
    </xs:complexType>

    <xs:element name="subscript">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
            </xs:choice>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="superscript">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="globalElements"/>
            </xs:choice>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="inlineequation" type="inlineequationInfo"/>
    <xs:complexType mixed="false" name="inlineequationInfo">
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:group ref="globalElements"/>
            <xs:element ref="mathphrase" />
        </xs:choice>
        <xs:attributeGroup ref="globalAttributes"/>
    </xs:complexType>

    <xs:element name="mathphrase" type="mathphraseType"/>
    <xs:complexType mixed="true" name="mathphraseType">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:group ref="globalElements"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="subscript"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="superscript"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="emphasis"/>
        </xs:choice>
        <xs:attributeGroup ref="globalAttributes"/>
    </xs:complexType>

    <xs:element name="informalfigure" type="informalfigureInfo"/>
    <xs:complexType mixed="false" name="informalfigureInfo">
        <xs:sequence>
            <xs:element ref="mediaobject"/>
        </xs:sequence>
        <xs:attribute name="label" type="xs:string"/>
        <xs:attribute name="target" type="xs:string"/>
        <xs:attribute name="role" type="xs:string"/>
        <xs:attribute name="float" type="xs:string"/>
        <xs:attributeGroup ref="globalAttributes"/>
    </xs:complexType>

    <xs:element name="figure" type="figureInfo"/>
    <xs:complexType mixed="false" name="figureInfo">
        <xs:sequence>
            <xs:element name="title" type="captionTitle"/>
            <xs:choice>
                <xs:element ref="mediaobject"/>
                <xs:element ref="informaltable"/>
            </xs:choice>
        </xs:sequence>
        <xs:attribute name="label" type="xs:string"/>
        <xs:attribute name="target" type="xs:string"/>
        <xs:attribute name="role" type="xs:string"/>
        <xs:attribute name="float" type="xs:string"/>
        <xs:attributeGroup ref="globalAttributes"/>
    </xs:complexType>

    <xs:element name="inlinemediaobject">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" name="imageobject">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="imagedata">
                                <xs:complexType>
                                    <xs:attribute name="fileref" use="required"/>
                                    <xs:attribute name="id" type="xs:ID"/>
                                    <xs:attribute name="xreflabel" type="xs:string"/>
                                    <xs:attribute name="width" type="xs:string"/>
                                    <xs:attribute name="contentwidth" type="xs:string"/>
                                    <xs:attribute name="depth" type="xs:string"/>
                                    <xs:attribute name="contentdepth" type="xs:string"/>
                                    <xs:attribute name="resizeTo" type="xs:string"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attributeGroup ref="globalAttributes"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="videoobject">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="videodata">
                                <xs:complexType>
                                    <xs:attribute name="fileref" use="required"/>
                                    <xs:attributeGroup ref="globalAttributes"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attributeGroup ref="globalAttributes"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="audioobject">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="audiodata">
                                <xs:complexType>
                                    <xs:attribute name="fileref" use="required"/>
                                    <xs:attributeGroup ref="globalAttributes"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attributeGroup ref="globalAttributes"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="role" type="xs:string"/>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>


    <xs:element name="mediaobject">
        <xs:complexType>
            <xs:sequence>
                <xs:element minOccurs="0" name="imageobject">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="imagedata">
                                <xs:complexType>
                                    <xs:attribute name="fileref" use="required"/>
                                    <xs:attribute name="id" type="xs:ID"/>
                                    <xs:attribute name="xreflabel" type="xs:string"/>
                                    <xs:attribute name="width" type="xs:string"/>
                                    <xs:attribute name="contentwidth" type="xs:string"/>
                                    <xs:attribute name="depth" type="xs:string"/>
                                    <xs:attribute name="contentdepth" type="xs:string"/>
                                    <xs:attribute name="resizeTo" type="xs:string"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attributeGroup ref="globalAttributes"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="videoobject">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="videodata">
                                <xs:complexType>
                                    <xs:attribute name="fileref" use="required"/>
                                    <xs:attributeGroup ref="globalAttributes"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attributeGroup ref="globalAttributes"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="audioobject">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="audiodata">
                                <xs:complexType>
                                    <xs:attribute name="fileref" use="required"/>
                                    <xs:attributeGroup ref="globalAttributes"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attributeGroup ref="globalAttributes"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" ref="caption"/>
            </xs:sequence>
            <xs:attribute name="role" type="xs:string"/>
            <xs:attributeGroup ref="globalAttributes"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="caption">
        <xs:complexType mixed="true">
            <xs:choice maxOccurs="unbounded" minOccurs="0">
                <xs:group ref="globalElements"/>
